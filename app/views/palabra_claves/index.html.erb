<p id="notice"><%= notice %></p>

<h1>Listing Palabra Claves</h1>

<table>
  <thead>
    <tr>
      <th>Palabra clave</th>
      <th>Palabra</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @palabra_claves.each do |palabra_clafe| %>
      <tr>
        <td><%= palabra_clafe.palabra_clave_id %></td>
        <td><%= palabra_clafe.palabra %></td>
        <td><%= link_to 'Show', palabra_clafe %></td>
        <td><%= link_to 'Edit', edit_palabra_clafe_path(palabra_clafe) %></td>
        <td><%= link_to 'Destroy', palabra_clafe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Palabra clave', new_palabra_clafe_path %>
